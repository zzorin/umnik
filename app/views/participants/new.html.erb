<%= simple_form_for @participant, url: :contest_participants do |f| %>
  <%= f.input :surname %>
  <%= f.input :name %>
  <%= f.input :patronymic %>
  <%= f.input :organization %>
  <%= f.input :project_title %>
  <%= f.input :project_info %>
  <%= f.input :contact_info %>
  <%= f.association :nomination %>
  <%= f.button :submit, class: 'btn-green' %>
<% end %>

<%= link_to 'Отменить', contest_participants_path, class: 'btn btn-light-blue' %>
